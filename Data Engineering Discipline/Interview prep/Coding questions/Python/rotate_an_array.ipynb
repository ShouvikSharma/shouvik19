   "source": [
    "print(arr)\n",
    "i = 0\n",
    "j = n - 1\n",
    "while i != j:\n",
    "    print(arr[i])\n",
    "    print(arr[j])\n",
    "    arr[i] , arr[j] = arr[j] , arr[i]\n",
    "    i = i+1\n",
    "\n",
    "print(arr)"
   ]
